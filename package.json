{
  "name": "x-dispatcher",
  "version": "1.0.0",
  "description": "JavaScript event dispatcher",
  "main": "./node/index.js",
  "browser": "./umd/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build:esm": "babel --config-file ./esm/.babelrc ./src/x-dispatcher.esm.js --out-file ./esm/index.js",
    "build:umd": "babel --config-file ./umd/.babelrc ./src/x-dispatcher.js --out-file ./umd/index.js",
    "build:node": "babel --config-file ./node/.babelrc ./src/x-dispatcher.js --out-file ./node/index.js",
    "build:all": "npm run lint:fix&& npm run build:umd&& npm run build:esm&& npm run build:node",
    "lint": "./node_modules/.bin/eslint  --ext .js src",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "anon",
  "license": "CC-BY-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anonym101/x-dispatcher"
  },
  "homepage": "https://eaglex.net",
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "contributors": [],
  "keywords": [
    "umd support",
    "browser support",
    "esm support",
    "dispatch event",
    "subscribe event",
    "event target",
    "request event",
    "callback hell",
    "custom events",
    "code hierarchy",
    "clean code",
    "x-dispatcher",
    "anon"
  ]
}
